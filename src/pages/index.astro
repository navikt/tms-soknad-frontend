---
import Layout from '../layouts/Layout.astro';
import type { Language } from '../language/language';
import { getAlleSoknaderUrl } from '@src/utils/server/urls';
import type { SoknadsData } from "@src/components/soknad/SoknadType";
import AlleSoknader from "@src/components/alle-soknader/AlleSoknader.tsx"
import { Link } from '@navikt/ds-react';

const soknader: SoknadsData[] = await fetch(getAlleSoknaderUrl)
  .then((response) => response.json())
  .then((data) => data);

const language = Astro.currentLocale as Language;
---

<Layout>
  <AlleSoknader soknader={soknader}/>
</Layout>
